<%# DISPLAY PAGE OF BOTH BOOKMARKS AND TAGS %>

<%- include("header") -%>
<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
    <div class="box" id="heading">
      <h1> <%=listTitle%> </h1>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
    <div class="box">
      <% listdata.forEach(function(item){ %>
        <form action="/delete" method="post">
          <div class="item">
            <a style="color:#0e615c;"href="<%=item%>"><%= item %> </a><br>
          </div>
        </form>
        <% }); %>
      </div>
    </div>
  </div>
  <%- include("footer") -%>
